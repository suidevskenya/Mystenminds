{
    "manifest_version": 3,
    "name": "MystenMinds AI Chat",
    "version": "1.0",
    "description": "Chat with MystenMinds AI agent about the Sui blockchain.",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "host_permissions": [
      "http://localhost:8000/*"
    ]
  }
  